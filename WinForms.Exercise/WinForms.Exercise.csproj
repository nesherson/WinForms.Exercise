  </ItemGroup>

  <ItemGroup>
    <None Remove="Ispit01.02.2024\rptGradovi.rdlc" />
    <None Remove="Ispit30.01.2023\rptUvjerenje.rdlc" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Ispit01.02.2024\rptGradovi.rdlc" />
